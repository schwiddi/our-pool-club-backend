{
  "name": "mule",
  "license": "UNLICENSED",
  "scripts": {
    "build:dev": "webpack --progress --config webpack.dev.config.babel.js",
    "build:prod": "webpack -p --config webpack.prod.config.babel.js",
    "dev": "webpack-dev-server --progress --config webpack.dev.config.babel.js",
    "dev:bart": "webpack-dev-server --progress --config webpack.bart.config.babel.js",
    "dev:sauterm": "webpack-dev-server --progress --config webpack.sauterm.config.babel.js",
    "lint": "eslint --ext .jsx --ext .js  src/",
    "lint-fix": "eslint --fix --ext .jsx --ext .js  src/",
    "deps:check": "depcheck --ignores=babel-* --ignore-dirs=dist,coverage .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test-go": "PATH=/usr/local/go/bin:${PATH} go test ../...",
    "vet-go": "PATH=/usr/local/go/bin:${PATH} go vet ../...",
    "prepush": "yarn test && yarn test-go && yarn vet-go",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "jest": {
    "coverageReporters": [
      "html"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testPathIgnorePatterns": [
      "/cypress/"
    ]
  },
  "dependencies": {
    "ansi-to-react": "^2.0.1",
    "bootstrap": "^3.3.7",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "normalizr": "^3.2.4",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^16.0.0",
    "react-notification-system": "^0.2.15",
    "react-notification-system-redux": "^1.1.6",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.2.2",
    "react-table": "^6.6.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.1.0",
    "uuid": "^3.1.0",
    "viz.js": "^1.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-idx": "^2.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "cypress": "^3.1.4",
    "deep-freeze": "^0.0.1",
    "depcheck": "^0.6.8",
    "esformatter": "^0.10.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^1.1.5",
    "git-revision-webpack-plugin": "^2.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-cli": "^21.2.1",
    "nock": "^9.0.14",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "wait-on": "^3.2.0",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  }
